{
  "name": "minute-maker",
  "version": "0.0.0",
  "description": "Minute Maker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mflorence99/minute-maker.git"
  },
  "author": "Mark Florence",
  "main": "./dist/main/app/electron.js",
  "scripts": {
    "build": "rm -rf dist && npm run build:main && npm run build:renderer",
    "build:main": "tsc -p main/tsconfig.app.json",
    "build:renderer": "ng build --configuration production",
    "clean:config": "rm -f ~/.config/minute-maker/config.dev.json",
    "clean:deps": "rm -rf node_modules && rm -f package-lock.json && npm install && npm outdated",
    "clean:jest": "jest --clearCache",
    "deb64:installer": "electron-installer-debian --src package/ternimal-linux-x64/ --arch amd64 --config electron-installer.json",
    "deb64:package": "npm run build && rm -rf temp && mkdir temp && cp -R dist/ temp/dist && cp -R node_modules/ temp/ && cp package.json temp/ && rm -rf package && electron-packager ./temp --out=package",
    "eslint": "eslint --ext ts --fix ./main ./renderer",
    "lint": "npm run eslint && npm run prettier && npm run stylelint",
    "postinstall": "rm -rf node_modules/@types/rimraf",
    "prettier": "prettier --write .",
    "serve:main": "npm run build:main -- --incremental && electron .",
    "serve:renderer": "ng serve",
    "show:config": "cat ~/.config/minute-maker/config.dev.json",
    "stylelint": "stylelint --fix ./renderer/**/*.{less,ts}",
    "test": "npm run test:main && npm run test:renderer",
    "test:main": "jest main --config main/jest.config.json --detectOpenHandles --silent --verbose",
    "test:main:only": "jest --config main/jest.config.json --coverage=false -- %1",
    "test:renderer": "jest renderer --config renderer/jest.config.json --detectOpenHandles --silent --verbose",
    "test:renderer:only": "jest --config renderer/jest.config.json --coverage=false -- %1"
  },
  "dependencies": {
    "@google-cloud/speech": "^5.5.0",
    "@google-cloud/storage": "^6",
    "@sentry/node": "^7",
    "@taiga-ui/icons": "^3",
    "electron-context-menu": "^3",
    "electron-is-dev": "^2",
    "electron-squirrel-startup": "^1",
    "electron-store": "^8",
    "exponential-backoff": "^3",
    "jsome": "^2",
    "music-metadata": "^7",
    "openai": "^3",
    "sharp": "^0.32.2",
    "zod": "^3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16",
    "@angular-eslint/builder": "^16",
    "@angular-eslint/eslint-plugin": "^16",
    "@angular-eslint/eslint-plugin-template": "^16",
    "@angular-eslint/template-parser": "^16",
    "@angular/animations": "^16",
    "@angular/cdk": "^16",
    "@angular/cli": "^16",
    "@angular/common": "^16",
    "@angular/compiler": "^16",
    "@angular/compiler-cli": "^16",
    "@angular/core": "^16",
    "@angular/forms": "^16",
    "@angular/platform-browser": "^16",
    "@angular/platform-browser-dynamic": "^16",
    "@angular/router": "^16",
    "@ng-web-apis/common": "^3",
    "@ngxs/devtools-plugin": "^3",
    "@ngxs/logger-plugin": "^3",
    "@ngxs/router-plugin": "^3",
    "@ngxs/storage-plugin": "^3",
    "@ngxs/store": "^3",
    "@sentry/angular-ivy": "^7",
    "@sentry/browser": "^7",
    "@sentry/tracing": "^7",
    "@taiga-ui/cdk": "^3",
    "@taiga-ui/core": "^3",
    "@taiga-ui/kit": "^3",
    "@taiga-ui/layout": "^3",
    "@taiga-ui/styles": "^3",
    "@types/jest": "^29",
    "@types/marked": "^5",
    "@types/nunjucks": "^3",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "dayjs": "^1",
    "electron": "^25",
    "electron-installer-debian": "^3",
    "electron-packager": "^17",
    "eslint": "^8",
    "eslint-config-mflorence99": "^1",
    "eslint-config-prettier": "^8",
    "eslint-plugin-import": "^2",
    "eslint-plugin-import-splitnsort": "^1",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-jest-extended": "^2",
    "eslint-plugin-prefer-arrow": "^1",
    "eslint-plugin-prettier": "^5",
    "file-saver": "^2",
    "jest": "^29",
    "jest-extended": "^4",
    "jest-junit": "^16",
    "jest-preset-angular": "^13",
    "marked": "^4",
    "material-design-icons": "^3",
    "ng-mocks": "^14",
    "ngx-autosize": "^2.0.4",
    "ngx-markdown": "^16",
    "nunjucks": "^3",
    "postcss": "^8",
    "postcss-angular": "^1",
    "postcss-less": "^6",
    "prettier": "2.8.8",
    "prettier-plugin-css-order": "^1",
    "prettier-plugin-package": "^1",
    "rxjs": "^7",
    "stylelint": "^15",
    "stylelint-config-standard": "^34",
    "stylelint-order": "^6",
    "ts-jest": "^29",
    "tslib": "^2",
    "typeface-roboto": "^1",
    "typescript": "^5",
    "uuid": "^9",
    "wavesurfer.js": "^7",
    "zone.js": "~0.13.1"
  }
}
