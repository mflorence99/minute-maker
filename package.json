{
  "name": "minute-maker",
  "version": "0.0.0",
  "description": "Minute Maker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mflorence99/minute-maker.git"
  },
  "author": "Mark Florence",
  "main": "./dist/main/electron.js",
  "scripts": {
    "build": "rm -rf dist && npm run build:main && npm run build:renderer",
    "build:main": "tsc -p main/tsconfig.json",
    "build:renderer": "ng build --configuration production",
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm install && npm outdated",
    "electron:main": "npm run build:main && DEV_MODE=1 electron .",
    "electron:renderer": "ng serve",
    "lint": "eslint --ext ts --fix ./main ./renderer",
    "prettier": "prettier --write .",
    "start": "ng serve",
    "test": "jest --detectOpenHandles --silent --verbose",
    "test:only": "jest --coverage=false -- %1"
  },
  "dependencies": {
    "@angular/animations": "^16",
    "@angular/cdk": "^16",
    "@angular/common": "^16",
    "@angular/compiler": "^16",
